<h1>All Categories</h1>

<ul>
  <% @all_categories.each do |category| %>
    <li>
      <%= image_tag category.image, class: 'category-image' if category.image.attached?%>
      <%= link_to category.name, user_category_expenses_path(user_id: current_user.id, category_id: category.id) %>
    </li>
  <% end %>
</ul>
